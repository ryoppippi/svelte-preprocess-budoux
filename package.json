{
	"name": "svelte-preprocess-budoux",
	"type": "module",
	"version": "1.1.2",
	"packageManager": "pnpm@10.19.0",
	"description": "svelte-preprocess plugin for Budoux",
	"author": "ryoppippi",
	"license": "MIT",
	"repository": "ryoppippi/svelte-preprocess-budoux",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"jsr": "./src/index.ts",
			"bun": "./src/index.ts",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"LICENSE",
		"README.md",
		"dist",
		"package.json",
		"src"
	],
	"jsrExclude": [
		"dist",
		"node_modules",
		"tests"
	],
	"scripts": {
		"build": "tsdown src --dts --sourcemap --publint --unused --unbundle",
		"typecheck": "tsc --noEmit",
		"lint": "eslint --cache .",
		"format": "nr lint --fix",
		"test": "vitest",
		"prerelease": "pnpm /^prerelease:/",
		"prerelease:typecheck": "nr typecheck",
		"prerelease:test": "nr test run",
		"prerelease:build": "nr build",
		"prerelease:jsr": "rm -rf jsr.json",
		"prepack": "nr build",
		"release": "nlx bumpp && nr release:npm && nr release:jsr",
		"release:npm": "npm publish",
		"release:jsr": "nlx pkg-to-jsr && nlx jsr publish --allow-dirty"
	},
	"peerDependencies": {
		"budoux": "^0.7.0",
		"svelte": ">= 5.42.2"
	},
	"dependencies": {
		"defu": "^6.1.4",
		"magic-string": "^0.30.21",
		"node-html-parser": "^7.0.1",
		"svelte-parse-markup": "^0.1.5",
		"zimmerframe": "^1.1.4"
	},
	"devDependencies": {
		"@antfu/ni": "^25.0.0",
		"@babel/types": "^7.28.5",
		"@ryoppippi/eslint-config": "npm:@jsr/ryoppippi__eslint-config@^0.0.28",
		"@sveltejs/kit": "^2.48.0",
		"@types/estree": "^1.0.8",
		"@types/node": "^24.9.1",
		"budoux": "^0.7.0",
		"eslint": "^9.38.0",
		"eslint-plugin-svelte": "^3.12.5",
		"publint": "^0.3.15",
		"svelte": "5.42.2",
		"svelte-eslint-parser": "^1.4.0",
		"tsdown": "^0.15.10",
		"typescript": "^5.9.3",
		"unplugin-unused": "^0.5.4",
		"vitest": "^3.2.4"
	}
}
