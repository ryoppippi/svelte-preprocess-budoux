{
	"name": "svelte-preprocess-budoux",
	"type": "module",
	"version": "1.1.0",
	"packageManager": "pnpm@10.10.0",
	"description": "svelte-preprocess plugin for Budoux",
	"author": "ryoppippi",
	"license": "MIT",
	"repository": "ryoppippi/svelte-preprocess-budoux",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"jsr": "./src/index.ts",
			"bun": "./src/index.ts",
			"import": "./dist/index.mjs"
		},
		"./package.json": "./package.json"
	},
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"LICENSE",
		"README.md",
		"dist",
		"package.json",
		"src"
	],
	"jsrExclude": [
		"dist",
		"node_modules",
		"tests"
	],
	"scripts": {
		"build": "mkdist -d",
		"typecheck": "tsc --noEmit",
		"lint": "eslint --cache .",
		"format": "nr lint --fix",
		"test": "vitest",
		"release": "nr release:pre && nlx bumpp && nr release:npm && nr release:jsr",
		"release:pre": "nr typecheck && nr test run && nr build && nlx publint",
		"release:npm": "npm publish",
		"release:jsr": "nlx pkg-to-jsr && nlx jsr publish --allow-dirty"
	},
	"peerDependencies": {
		"budoux": "^0.6.4",
		"svelte": ">= 5.28.2"
	},
	"dependencies": {
		"defu": "^6.1.4",
		"magic-string": "^0.30.17",
		"node-html-parser": "^7.0.1",
		"svelte-parse-markup": "^0.1.5",
		"zimmerframe": "^1.1.2"
	},
	"devDependencies": {
		"@antfu/ni": "^24.3.0",
		"@babel/types": "^7.27.1",
		"@ryoppippi/eslint-config": "npm:@jsr/ryoppippi__eslint-config@^0.0.23",
		"@sveltejs/kit": "^2.20.8",
		"@types/estree": "^1.0.7",
		"@types/node": "^22.15.16",
		"budoux": "^0.6.4",
		"eslint": "^9.14.0",
		"eslint-plugin-svelte": "^3.5.1",
		"mkdist": "^2.3.0",
		"svelte": "5.28.2",
		"svelte-eslint-parser": "^1.1.3",
		"typescript": "^5.8.3",
		"vitest": "^3.1.3"
	}
}
