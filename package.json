{
  "name": "svelte-preprocess-budoux",
  "version": "0.4.1",
  "description": "svelte-preprocess plugin for Budoux",
  "author": "ryoppippi",
  "repository": "ryoppippi/svelte-preprocess-budoux",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "module": "dist/index.mjs",
