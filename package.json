{
  "name": "svelte-preprocess-budoux",
  "version": "0.3.0",
  "description": "svelte-preprocess plugin for Budoux",
  "author": "ryoppippi",
  "repository": "ryoppippi/svelte-preprocess-budoux",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "mkdist -d",
    "typecheck": "tsc --noEmit",
    "release": "npm run typecheck && npm run build && changelogen --release && npm publish && git push --follow-tags",
    "fmt": "deno fmt"
  },
  "devDependencies": {
    "@babel/types": "^7.24.9",
    "@sveltejs/kit": "^2.5.18",
    "@types/estree": "^1.0.5",
    "changelogen": "^0.5.5",
    "mkdist": "^1.5.4",
    "node-html-parser": "^6.1.13",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "budoux": "^0.6.2",
    "defu": "^6.1.4",
    "estree-walker": "^3.0.3",
    "magic-string-ast": "^0.6.2",
    "svelte": "^5.0.0-next.186",
    "svelte-parse-markup": "^0.1.5"
  }
}
