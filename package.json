{
	"name": "svelte-preprocess-budoux",
	"type": "module",
	"version": "0.4.4",
	"packageManager": "pnpm@9.7.0",
	"description": "svelte-preprocess plugin for Budoux",
	"author": "ryoppippi",
	"license": "MIT",
	"repository": "ryoppippi/svelte-preprocess-budoux",
	"sideEffects": false,
	"exports": {
		".": {
			"bun": "./src/index.ts",
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs"
		}
	},
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "mkdist -d",
		"typecheck": "tsc --noEmit",
		"release": "nr typecheck && nr test run && nr build && nlx publint && nlx bumpp && npm publish",
		"lint": "eslint --cache .",
		"format": "nr lint --fix",
		"test": "vitest"
	},
	"dependencies": {
		"budoux": "^0.6.2",
		"defu": "^6.1.4",
		"magic-string": "^0.30.11",
		"svelte": "^5.0.0-next.210",
		"svelte-parse-markup": "^0.1.5"
	},
	"devDependencies": {
		"@antfu/ni": "^0.22.0",
		"@babel/types": "^7.25.2",
		"@ryoppippi/eslint-config": "npm:@jsr/ryoppippi__eslint-config",
		"@sveltejs/kit": "^2.5.20",
		"@types/estree": "^1.0.5",
		"@types/node": "^22.1.0",
		"eslint": "^9.8.0",
		"eslint-plugin-svelte": "^2.43.0",
		"mkdist": "^1.5.4",
		"node-html-parser": "^6.1.13",
		"svelte-eslint-parser": "^0.41.0",
		"typescript": "^5.5.4",
		"vitest": "^2.0.5",
		"zimmerframe": "^1.1.2"
	}
}
